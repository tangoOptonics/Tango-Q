<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/constraintLayout57"
    android:layout_width="match_parent"
    android:layout_height="160dp"
    android:background="@color/white">

    <FrameLayout
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:layout_gravity="right"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/ivEditDrag"
        android:layout_width="48dp"
        android:layout_height="48dp"
        android:layout_marginEnd="24dp"
        android:background="@color/white"
        android:padding="8dp"
        android:scaleType="centerCrop"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/icon_drag" />

    <TextView
        android:id="@+id/tvEditName"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="8dp"
        android:singleLine="true"
        android:text="목 관절 운동 프로그램"
        android:textColor="@color/black"
        android:textSize="22dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/ivEditDrag"
        app:layout_constraintStart_toEndOf="@+id/cardView12"
        app:layout_constraintTop_toTopOf="@+id/cardView12"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/tvEditSymptom"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        android:layout_marginEnd="4dp"
        android:singleLine="true"
        android:text="증상들 생략.."

        android:textColor="@color/subColor500"
        android:textSize="18dp"
        app:layout_constraintEnd_toStartOf="@+id/ivEditDrag"
        app:layout_constraintStart_toStartOf="@+id/tvEditName"
        app:layout_constraintTop_toBottomOf="@+id/tvEditName" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="28dp"
        android:layout_height="28dp"
        android:layout_marginBottom="4dp"
        app:layout_constraintBottom_toBottomOf="@+id/cardView12"
        app:layout_constraintStart_toStartOf="@+id/tvEditSymptom"
        app:layout_constraintTop_toBottomOf="@+id/tvEditSymptom"
        app:layout_constraintVertical_bias="0.9"
        app:srcCompat="@drawable/icon_rv_summary_time" />

    <ImageView
        android:id="@+id/ivEIStage"
        android:layout_width="28dp"
        android:layout_height="28dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView5"
        app:layout_constraintEnd_toStartOf="@+id/ivEditDrag"
        app:layout_constraintStart_toEndOf="@+id/cardView12"
        app:layout_constraintTop_toTopOf="@+id/imageView5"
        app:srcCompat="@drawable/icon_rv_summary_intensity" />

    <TextView
        android:id="@+id/tvEditTime"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:text="25"
        android:textColor="@color/subColor800"
        android:textSize="20dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView5"
        app:layout_constraintStart_toEndOf="@+id/imageView5"
        app:layout_constraintTop_toTopOf="@+id/imageView5" />

    <TextView
        android:id="@+id/tvEditIntensity"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:text="회복"
        android:textColor="@color/subColor800"
        android:textSize="20dp"
        app:layout_constraintBottom_toBottomOf="@+id/ivEIStage"
        app:layout_constraintStart_toEndOf="@+id/ivEIStage"
        app:layout_constraintTop_toTopOf="@+id/ivEIStage" />

    <com.google.android.material.divider.MaterialDivider
        android:layout_width="match_parent"
        android:layout_height="2dp"
        app:dividerColor="@color/subColor100"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <androidx.cardview.widget.CardView
        android:id="@+id/cardView12"
        android:layout_width="140dp"
        android:layout_height="0dp"
        android:layout_marginStart="40dp"
        android:layout_marginTop="12dp"
        android:layout_marginBottom="12dp"
        app:cardCornerRadius="8dp"
        app:cardElevation="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <ImageView
            android:id="@+id/ivEditThumbnail"
            android:layout_width="match_parent"
            android:layout_height="match_parent"

            android:clipToOutline="true"
            android:scaleType="centerCrop"
            app:srcCompat="@drawable/background_button" />
    </androidx.cardview.widget.CardView>
</androidx.constraintlayout.widget.ConstraintLayout>