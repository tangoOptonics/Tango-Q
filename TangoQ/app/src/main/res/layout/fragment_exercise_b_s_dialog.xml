            android:layout_height="wrap_content"
            android:layout_marginStart="10dp"
            android:layout_marginTop="4dp"
            android:singleLine="true"
            android:text="_"
            android:textColor="@color/black"
            android:textSize="18dp"
            android:textStyle="bold"
            app:layout_constraintEnd_toStartOf="@+id/ibtnEBsExit"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toEndOf="@+id/cardView27"
            app:layout_constraintTop_toTopOf="@+id/cardView27" />

        <TextView
            android:id="@+id/tvEBSSymptom"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="10dp"
            android:singleLine="true"
            android:text="_"
            android:textColor="@color/subColor500"
            app:layout_constraintBottom_toBottomOf="@+id/cardView27"
            app:layout_constraintEnd_toStartOf="@+id/ibtnEBsExit"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toEndOf="@+id/cardView27"
            app:layout_constraintTop_toBottomOf="@+id/tvEBSName" />

        <androidx.cardview.widget.CardView
            android:id="@+id/cardView27"
            android:layout_width="48dp"
            android:layout_height="48dp"
            android:layout_marginStart="32dp"
            android:layout_marginTop="20dp"
            app:cardCornerRadius="4dp"
            app:cardElevation="0dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <ImageView
                android:id="@+id/ivEBsThumbnail"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:src="@drawable/profile_default" />
        </androidx.cardview.widget.CardView>

        <ImageButton
            android:id="@+id/ibtnEBsExit"
            android:layout_width="36dp"
            android:layout_height="36dp"
            android:layout_marginEnd="16dp"
            android:background="@color/white"
            android:padding="4dp"
            android:scaleType="centerCrop"
            android:src="@drawable/icon_exit"
            app:layout_constraintBottom_toBottomOf="@+id/tvEBSName"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="@+id/tvEBSName" />

